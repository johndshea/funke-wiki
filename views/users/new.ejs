<div class="card ">
  <div class="card-content black-text">
<h5>Create new user</h5>
<form action="/users" method="post">
<div class="input-field col s12">
  <input type="email" id="email"
         name="user[email]"
         placeholder="ineedsleep@codingtillmyeyesbleed.com">
 <label for="email">Email</label>
</div>
<div class="input-field col s12">
  <input type="text" id="name"
         name="user[name]">
  <label for="name">Name</label>
</div>
<div class="input-field col s12">
  <input type="password" id="password"
         name="user[password]">
  <label for="password">Password</label>
</div>
<div class="input-field col s12">
  <input type="password" id="passwordVerification"
         name="user[passwordVerification]">
  <label for="passwordVerification">Confirm password</label>
</div>
<input type="submit" value="Sign Up">
</form>

<hr>
[<a href="/session/new">Sign In Instead</a>]
</div>
</div>
